{
	"name": "wuyan/yii2-aliyun-oss",
	"description": "Yii2 warper for aliyun oss",
	"type": "library",
	"require": {
		"php": ">=8.2",
		"aliyuncs/oss-sdk-php": "^2.7",
		"yiisoft/yii2": "~2.0.51"
	},
	"require-dev": {
		"alibabacloud/credentials": "^1.2"
	},
	"suggest": {
		"alibabacloud/credentials": "Alibaba Cloud credentials SDK"
	},
	"license": "GPL",
	"autoload": {
		"psr-4": {
			"yii\\aliyunoss\\": "src/"
		}
	},
	"authors": [
		{
			"name": "WU Yan",
			"email": "wooyen@163.com"
		}
	],
	"repositories": {
		"asset-packagist": {
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}
